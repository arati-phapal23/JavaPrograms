class VerticalShape 
{
	public static void main(String[] args) 
	{
		 int n =4;
		for(int i =1; i<=n; i++){

			for(int j =1; j<=n; j++){

				if(i==4 && j==4)

				  System.out.println("*"+ " ");
				else
				  System.out.println(" ");
				

			}


		System.out.println();
	}
}
}

